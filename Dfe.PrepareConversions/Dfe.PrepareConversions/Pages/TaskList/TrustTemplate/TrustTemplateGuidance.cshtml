@page "/task-list/{id:int}/template-guidance"
@model Dfe.PrepareConversions.Pages.BaseAcademyConversionProjectPageModel
@{
   ViewData["Title"] = "Prepare your trust template";
}

@section BeforeMain
{
   <govuk-back-link link-item="@Links.TaskList.Index" />
}
@if (@Model.Project.IsSponsored)
{
    @Html.Partial("_SponsoredTemplateGuidance")
    ;
}
else
{
    @Html.Partial("_TrustTemplateGuidance");
}
