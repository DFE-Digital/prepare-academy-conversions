@using Dfe.PrepareTransfers.Helpers
@using Dfe.PrepareTransfers.Web.Dfe.PrepareTransfers.Helpers.TagHelpers
@using Microsoft.AspNetCore.Mvc.TagHelpers
@model Dfe.PrepareTransfers.Web.Pages.Projects.LatestOfstedJudgement.Index

<dl class="govuk-summary-list">
    @if (Model.LatestOfstedJudgement.LatestInspectionIsSection8)
    {
        <gds-key-value key="Latest short inspection date" value="@DatesHelper.DateStringToGovUkDate(Model.LatestOfstedJudgement.DateOfLatestSection8Inspection)" show-action=true />
    }

    <gds-key-value key="Overall effectiveness" value='@Model.LatestOfstedJudgement.OverallEffectiveness' show-action=true />
    <partial name="~/Areas/Transfers/Pages/Shared/OfstedSummary/_SummaryRows.cshtml" for="@Model"></partial>
</dl>