# EditorConfig for Cypress Tests
# https://editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = crlf
indent_style = space
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true

[*.{ts,tsx,js,jsx}]
indent_size = 4

[*.json]
indent_size = 4

[*.{yaml,yml}]
indent_size = 2

[*.md]
trim_trailing_whitespace = false
