@page "/task-list/{id:int}/confirm-school-trust-information-project-dates/previous-advisory-board-date-question"
@model ApplyToBecomeInternal.Pages.TaskList.SchoolAndTrustInformation.PreviousHeadTeacherBoardDateQuestion
@{
	Layout = "_QuestionLayout";
	Model.SuccessPage = Links.SchoolAndTrustInformationSection.PreviousHeadTeacherBoardDate.Page;
	ViewData["Title"] = "Has this project been to a previous Advisory Board?";
	var values = new[] { "Yes", "No" };
}

@section BeforeMain
{
	<govuk-back-link link-item="@Links.SchoolAndTrustInformationSection.ConfirmSchoolAndTrustInformation" />
}

<govuk-radios-input heading-label="true" label="Has this project been to a previous Advisory Board?" id="previous-head-teacher-board-date-question" name="previous-head-teacher-board-date-question" asp-for="Project.PreviousHeadTeacherBoardDateQuestion" values="@values" labels="@values" />
